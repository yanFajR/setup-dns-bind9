;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ryan.fajri. root.ryan.fajri. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	  IN	NS	ryan.fajri.
77	IN	PTR	ryan.fajri.
77  IN	PTR	www.ryan.fajri.
77	IN	PTR	ftp.ryan.fajri.
77	IN	PTR	mail.ryan.fajri.
77	IN	PTR	host.ryan.fajri.
